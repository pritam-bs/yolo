<manifest xmlns:android="http://schemas.android.com/apk/res/android">
  
  <uses-permission android:name="android.permission.CAMERA" />
  
  <uses-feature
      android:name="android.hardware.camera"
      android:required="false" />
  <uses-feature
      android:name="android.hardware.camera.autofocus"
      android:required="false" />

  <application>
    <meta-data
        android:name="com.google.android.gms.tflite.auto_migration_resumption"
        android:value="true" />
    <meta-data
        android:name="com.google.android.gms.tflite.optional_module_inference"
        android:value="tflite_dynamite,tflite_gpu" />
  </application>
</manifest>
